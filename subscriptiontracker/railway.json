{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd subscriptiontracker && ./mvnw clean package -DskipTests"
  },
  "deploy": {
    "startCommand": "java -jar subscriptiontracker/target/subscriptiontracker-0.0.1-SNAPSHOT.jar",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
